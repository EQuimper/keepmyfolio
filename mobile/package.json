    "expo": "^20.0.0",
    "glamorous-native": "^1.1.2",
    "idx": "^1.5.0",
    "invariant": "^2.2.2",
    "react": "16.0.0-alpha.12",
    "react-native": "https://github.com/expo/react-native/archive/sdk-20.0.0.tar.gz",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.6",
    "react-relay": "^1.3.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "relay-runtime": "^1.3.0",
    "styled-components": "^2.1.2"
