    "flow-bin": "0.49.1",
    "jest-expo": "^20.0.0",
    "prettier": "^1.6.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "relay-compiler": "^1.3.0"
  }
