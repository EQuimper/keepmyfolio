{
  "name": "keepmyfolio-api",
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "yarn lint",
      "git add"
    ]
  },
  "scripts": {
    "build:watch": "babel -w --out-dir=dist --ignore=__tests__/* ./crypto",
    "prettier": "prettier --single-quote --print-width 80 --trailing-comma all --write 'src/**/*.js'",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "eslint": "^4.6.1",
    "eslint-config-equimper": "^2.2.1",
    "eslint-config-prettier": "^2.4.0",
    "flow-bin": "^0.54.1",
    "husky": "^0.14.3",
    "isomorphic-fetch": "^2.2.1",
    "lint-staged": "^4.1.2",
    "prettier": "^1.6.1",
    "serverless-offline": "^3.15.3",
    "serverless-plugin-graphiql": "^0.3.0"
  },
  "dependencies": {
    "cross-env": "^5.0.5",
    "graphql": "^0.11.3",
    "graphql-relay": "^0.5.2",
    "invariant": "^2.2.2"
  }
}
